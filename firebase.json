{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "build",
    "rewrites": [{
      "source": "**",
      "destination": "/index.html"
    }],
    "ignore": [
      "bower.json",
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "README.md"
    ]
  }
}
